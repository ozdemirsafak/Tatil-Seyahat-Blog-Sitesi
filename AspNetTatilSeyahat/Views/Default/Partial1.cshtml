@using AspNetTatilSeyahat.Models.Siniflar
@model List<Blog>


    @foreach (var x in Model)
    {


        <div class="left">
            <div class="bot-left">
                <img src="@x.BlogImage" alt="" />
            </div>
            <div class="left-text">
                <h4>@x.Baslik</h4>
                <span class="doller">@x.Tarih.ToString("dd.MM.yyyy") </span>
                <p>@x.Aciklama</p>
                <p><a href="/Blog/BlogDetay/@x.Id"><span class="read">Devamın Oku. >></span></a></p>
            </div>
        </div>
    }
    @Html.Action("Partial2", "Default")
    <div class="clear"></div>
